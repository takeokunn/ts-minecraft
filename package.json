{
  "name": "ts-minecraft",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "editorconfig": "editorconfig-checker",
    "check": "pnpm typecheck && pnpm format:check && pnpm editorconfig",
    "check:fix": "pnpm format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "fix:extensions": "bash scripts/remove-extensions.sh",
    "validate:imports": "tsx scripts/validate-imports.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@effect/platform": "^0.90.10",
    "@effect/schema": "^0.75.5",
    "@fp-ts/optic": "^0.25.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "cannon-es": "^0.20.0",
    "effect": "^3.18.2",
    "framer-motion": "^12.23.22",
    "idb-keyval": "^6.2.2",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.180.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cannon": "^0.1.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.6.2",
    "@types/three": "^0.180.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "editorconfig-checker": "^6.1.0",
    "glob": "^11.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
